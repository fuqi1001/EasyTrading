<div class="jumbotron" style="background: white">
	<div class="container">
		<h4>Publish New Apartment</h4>
		<div class="row">
			<div class="col-sm-12">
				<form id="new-apartment-form">
					<div class="form-group">
						<input id="new-apartment-name" type="text" tabindex="1" class="form-control" placeholder="Apartment address" value="">
					</div>
					<div class="form-group">
						<select class="form-control" id="new-apartment-location" name="location" tabindex="2">
                        {{#each mylocation}} {{> location_item}} {{/each}}
                    </select>
					</div>
					<div class="form-group">
						<input id="new-apartment-addressdetail" type="text" tabindex="3" class="form-control" placeholder="Apartment address detail"
							value="">
					</div>
					<div class="form-group">
						<div class="input-group input-group-md">
							<span class="input-group-addon glyphicon glyphicon-usd"></span>
							<input id="new-apartment-price" type="text" tabindex="4" class="form-control" placeholder="Price" value="">
						</div>
					</div>
					<div class="form-group">
						<textarea id="new-apartment-description" type="text" tabindex="5" class="form-control" placeholder="Apartment description"
							value=""></textarea>
					</div>
					<div class="form-group">
						<input type="submit" value="Publish New Apartment" class="form-control btn btn-login">
					</div>
				</form>
				<div class="alert alert-danger hidden" id="form-alert"></div>
				<button id="user-log-out" class="btn btn-default">User Log Out</button>
                <button class="btn btn-default"><a href="/">back to homepage</a></button>
			</div>
		</div>
	</div>
</div>