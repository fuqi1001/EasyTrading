<div class="jumbotron" style="background: #f2f2f2">
	<div class="container ">
		<div class="row  ">
			<div class="col-sm-12 text-col text-center">
				<section>
					<div class="row">
						{{#each myapartment}}
						<div class="col-sm-6 col-md-4" sytle="float:left">
							<div class="thumbnail">

								<div class="caption">
									<h3>{{this.name}}</h3>
									<p>${{this.price}}</p>
									<p><a href="/apartments/{{this._id}}" class="btn btn-primary" role="button">check detail</a></p>
								</div>
							</div>
						</div>
						{{/each}}
					</div>
				</section>
			</div>
		</div>
	</div>
</div>
